#include <stdio.h>

int main()
{
    char palabra[50];
    int i;
    
    printf("Hola! Introduzca una palabra random" "\n");
    scanf ("%s", palabra);
    for (i=0; i<50; i++){
        if (palabra[i]=='\0') break;
    }

    printf("La cantidad de caracteres es que posee son: %d", i);
    return 0;
}
